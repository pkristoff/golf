<h1><%= Heading::Account::ACCOUNTS %></h1>

<% if @accounts.empty? %>
  <h2><%= Heading::Account::NO_ACCOUNTS %></h2>
<% else %>
  <ul>
    <% @accounts.each do |account| %>
      <li id="li-id"><%= link_to account.name, edit_account_path(account), id: "edit_link_#{account.id}" %></li>
    <% end %>
    <br>
  </ul>
<% end %>

<%= render "layouts/other_buttons", courses:
  [],
           rounds: [] %>