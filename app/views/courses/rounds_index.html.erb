<h1><%= I18n.t('button.round.choose_round_course') %></h1>

<ul id="courses">
  <% @courses.each do |course| %>
    <li id="li-id<%= course.id %>"><%= link_to course.name, rounds_tees_course_tees_path(course) %></li>
  <% end %>
</ul>

<%= render 'layouts/other_buttons',
           courses: [button_to(I18n.t('button.course.new'), new_course_path(Course.new), method: :get)
           ], rounds: []
%>